#include <iostream>
using namespace std;

int main() 
    float vegetablePrice, fruitPrice;
    int vegetableKg, fruitKg;
     float coinToRupees = 1.94; 
    cout << "Enter vegetable price per kilogram (in coins): ";
    cin >> vegetablePrice;
    cout << "Enter fruit price per kilogram (in coins): ";
    cin >> fruitPrice;
    cout << "Enter total kilograms of vegetables: ";
    cin >> vegetableKg;
    cout << "Enter total kilograms of fruits: ";
    cin >> fruitKg;
    float totalEarningsCoins = (vegetablePrice * vegetableKg) + (fruitPrice * fruitKg);
    float totalEarningsRps = totalEarningsCoins / coinToRupees 
   cout << "Total earnings in Rupees: " << totalEarningsRps << endl;

    
}
